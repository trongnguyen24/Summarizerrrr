{
  "name": "Summarizerrrr",
  "description": "Extension helps you summarize for YouTube, Udemy, Coursera, any websites. Crafted with superior UI/UX design.",
  "private": true,
  "version": "2.2.9",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare",
    "android": "web-ext run --target=firefox-android --android-device ${ANDROID_DEVICE:-emulator-5554} --firefox-apk ${FIREFOX_APK:-org.mozilla.fenix} --source-dir .output/firefox-mv2 --adb-remove-old-artifacts",
    "android:win": "web-ext run --target=firefox-android --android-device 177a8d45 --firefox-apk org.mozilla.fenix --source-dir .output/firefox-mv2 --adb-remove-old-artifacts"
  },
  "devDependencies": {
    "@ai-sdk/svelte": "^3.0.0",
    "@iconify/svelte": "^5.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tsconfig/svelte": "^5.0.4",
    "@wxt-dev/module-svelte": "^2.0.3",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.6",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "wxt": "^0.20.6"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.0",
    "@ai-sdk/deepseek": "^1.0.0",
    "@ai-sdk/google": "^2.0.0",
    "@ai-sdk/groq": "^2.0.0",
    "@ai-sdk/openai": "^2.0.0",
    "@ai-sdk/openai-compatible": "^1.0.0",
    "@fontsource-variable/geist-mono": "^5.2.6",
    "@fontsource-variable/noto-serif": "^5.2.7",
    "@fontsource/mali": "^5.2.6",
    "@fontsource/opendyslexic": "^5.2.5",
    "@humanspeak/svelte-markdown": "^0.8.11",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@tailwindcss/vite": "^4.1.8",
    "ai": "^5.0.39",
    "ai-sdk-ollama": "^0.6.0",
    "animejs": "^4.0.2",
    "bits-ui": "^1.4.3",
    "crypto-js": "^4.2.0",
    "highlight.js": "^11.11.1",
    "highlightjs-svelte": "^1.0.6",
    "marked": "^15.0.11",
    "ollama-ai-provider-v2": "^1.3.1",
    "openai": "^5.10.2",
    "overlayscrollbars": "^2.11.1",
    "overlayscrollbars-svelte": "^0.5.5",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "svelte-i18n": "^3.7.1",
    "svelte-markdown": "^0.4.1",
    "svelte-sonner": "^1.0.5",
    "tailwindcss": "^4.1.8",
    "webextension-polyfill": "^0.12.0"
  }
}
